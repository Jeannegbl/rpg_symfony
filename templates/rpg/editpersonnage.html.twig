{% extends 'base.html.twig' %}

{% block title %}{% for personnage in personnages %}{{personnage.nom}}{% endfor %}{% endblock %}

{% block javascripts %}
    <script src="AvataaarsJS-main/avataaars.js"></script>
{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div>
    {{form(form)}}
    {{form(form2)}}
    {% for personnage in personnages %}
        <input type="button" value="Retour" onclick="window.location.href='/character/{{personnage.id}}';">
    {% endfor %}
</div>
{% endblock %}